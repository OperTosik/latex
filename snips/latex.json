{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Theorem": {
		"prefix": "th",
		"body": [
			"\\begin{theorem}",
			"\t$0",
			"\\end{theorem}"
		]
	},
	"Subheorem": {
		"prefix": "subth",
		"body": [
			"\\begin{subtheorem}",
			"\t$0",
			"\\end{subtheorem}"
		]
	},
	"Definition": {
		"prefix": "def",
		"body": [
			"\\begin{definition}",
			"\t$0",
			"\\end{definition}"
		]
	},
	"Consequense": {
		"prefix": "con",
		"body": [
			"\\begin{consequense}",
			"\t$0",
			"\\end{consequense}"
		]
	},
	"Lemma": {
		"prefix": "lemma",
		"body": [
			"\\begin{lemma}",
			"\t$0",
			"\\end{lemma}"
		]
	},
	"Remark": {
		"prefix": "remark",
		"body": [
			"\\begin{remark}",
			"\t$0",
			"\\end{remark}"
		]
	},
	"proof": {
		"prefix": "proof",
		"body": [
			"\\begin{proof}",
			"\t$0",
			"\\end{proof}"
		]
	},
	"mk": {
		"prefix": "mk",
		"body": [
			"$$0$"
		]
	},
	"Tenzor": {
		"prefix": "ten",
		"body": [
			"T_{i_1,...,i_p}^{j_1,...,j_q}e^{i_1}\\otimes ... \\otimes e^{i_p} \\otimes e_{j_1} \\otimes ... \\otimes e_{j_q} $0"
		]
	},
	"lrangle": {
		"prefix": "<>",
		"body": [
			"\\langle$0\\rangle"
		]
	},
	"WideTilde": {
		"prefix": "wt",
		"body": [
			"\\widetilde{$0}"
		]
	},
	"OverLine": {
		"prefix": "ov",
		"body": [
			"\\overline{$0}"
		]
	},
	"Longmk": {
		"prefix": "lmk",
		"body": [
			"$$$0$$"
		]
	},
	"fraction": {
		"prefix": "frac",
		"body": [
			"\\frac{$1}{$2} $0"
		]
	},
	"Textbf": {
		"prefix": "bf",
		"body": [
			"\\textbf{${1:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"Textit": {
		"prefix": "it",
		"body": [
			"\\textit{${1:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"underbrace": {
		"prefix": "underbrace",
		"body": [
			"\\underbrace{${1:${TM_SELECTED_TEXT}}}_{$2} $0"
		]
	},
	"overbrace": {
		"prefix": "overbrace",
		"body": [
			"\\overbrace{${1:${TM_SELECTED_TEXT}}}_{$2} $0"
		]
	},
	"underset": {
		"prefix": "underset",
		"body": [
			"\\underset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"overset": {
		"prefix": "overset",
		"body": [
			"\\overset{$1}{${2:${TM_SELECTED_TEXT}}} $0"
		]
	},
	"itemize": {
		"prefix": "itemize",
		"body": [
			"\\begin{itemize}",
			"\t\\item $1",
			"\\end{itemize}",
			"$0"
		]
	},
	"enumerate": {
		"prefix": "enum",
		"body": [
			"\\begin{enumerate}",
			"\t\\item $1",
			"\\end{enumerate}",
			"$0"
		]
	},
	"Environment": {
		"prefix": "begin",
		"body": [
			"\\begin{${1:name}}",
			"\t$0",
			"\\end{${1:name}}"
		]
	},
	"Text": {
		"prefix": "text",
		"body": [
			"\\text{$0}"
		]
	},
	"Image": {
		"prefix": "img",
		"body": [
			"\\begin{figure}[H]",
			"\t\\centering",
			"\t\\includegraphics[width=0.5\\textwidth]{${1:${TM_SELECTED_TEXT}}}",
			"\t\\caption{${2:caption}\\label{fig:${3:sample}}}",
			"\\end{figure}",
			"$0"
		]
	},
	"Table": {
		"prefix": "tab",
		"body": [
			"\\begin{table}[H]",
			"\t\\centering",
			"\t\\begin{tabular}{${1:ccc}}",
			"\t\t\\toprule",
			"\t\ta & b & c \\\\\\ \\midrule",
			"\t\t1 & 2 & 3 \\\\\\",
			"\t\t\\bottomrule",
			"\t\\end{tabular}",
			"\t\\caption{${2:caption}\\label{tab:${3:sample}}}",
			"\\end{table}",
			"$0"
		]
	},
	"cases": {
		"prefix": "cases",
		"body": [
			"\\begin{cases}",
			"$1",
			"\\end{cases}",
			"$0"
		]
	},
	"vmatrix": {
		"prefix": "vmatrix",
		"body": [
			"\\begin{vmatrix}",
			"\t$0",
			"\\end{vmatrix}"
		]
	},
	"pmatrix": {
		"prefix": "pmatrix",
		"body": [
			"\\begin{pmatrix}",
			"\t$0",
			"\\end{pmatrix}"
		]
	},
	"paragraph": {
		"prefix": "paragraph",
		"body": [
			"\\begin{center}",
				"\t\\begin{Large}",
					"\t\t\\textbf{$0}",
				"\t\\end{Large}",
			"\\end{center}"
		]
	},
	"Integral": {
		"prefix": "int",
		"body": [
			"\\int\\limits_{${1:a}}^{${2:\\b}} $0"
		]
	},
	"undersetinline": {
		"prefix": "_",
		"body": [
			"_{$1}$0"
		]
	},
	"parenthesis1": {
		"prefix": "(",
		"body": [
			"\\left($1\\right"
		]
	},
	"parenthesis2": {
		"prefix": "[",
		"body": [
			"\\left[$1\\right"
		]
	},
	"parenthesis3": {
		"prefix": "{",
		"body": [
			"\\left{$1\\right"
		]
	},
	"average": {
		"prefix": "avg",
		"body": [
			"\\langle $1 \\rangle$0"
		]
	},
	"Sum": {
		"prefix": "sum",
		"body": [
			"\\sum\\limits_{${1:i=0}}^{${2:n}} $0"
		]
	},
	"prod": {
		"prefix": "prod",
		"body": [
			"\\prod_{${1:i=1}}^{${2:n}}$0"
		]
	},
	"hbar": {
		"prefix": "hb",
		"body": [
			"\\hbar"
		]
	},
	"section": {
		"prefix": "sec",
		"body": [
			"\\section{$1}",
			"$0"
		]
	},
	"subsection": {
		"prefix": "ssec",
		"body": [
			"\\subsection{$1}",
			"$0"
		]
	},
	"operatorname": {
		"prefix": "op",
		"body": [
			"\\operatorname{$1} $0",
		]
	},
	"mathrm": {
		"prefix": "rm",
		"body": [
			"\\mathrm{$1} $0"
		]
	},
	"mathbb": {
		"prefix": "mb",
		"body": [
			"\\mathbb{$1} $0"
		]
	},
	"mathcal": {
		"prefix": "mc",
		"body": [
			"\\mathcal{$1} $0"
		]
	},
	"widetilde": {
		"prefix": "wt",
		"body": [
			"\\widetilde{$1} $0"
		]
	},
	"underline": {
		"prefix": "underline",
		"body": [
			"\\underline{$1} $0"
		]
	},
	"Longrightarrow": {
		"prefix": "ra",
		"body": [
			"\\Longrightarrow $0"
		]
	},
	"Longlefttarrow": {
		"prefix": "la",
		"body": [
			"\\Longleftarrow $0"
		]
	},
	"Longleftrightarrow": {
		"prefix": "lra",
		"body": [
			"\\Longleftrightarrow $0"
		]
	},
	"mapsto": {
		"prefix": "map",
		"body": [
			"\\mapsto $0"
		]
	},
	"longmapsto": {
		"prefix": "lmap",
		"body": [
			"\\longmapsto $0"
		]
	},
	"infinity": {
		"prefix": "inf",
		"body": [
			"\\infty $0"
		]
	}
}